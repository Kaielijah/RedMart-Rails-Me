<% provide(:title, @user.name) %>

<div class="container container-others">
  <div class="row">

<div class="col-md-3 col-md-offset-3">
  <%= gravatar_for @user %>

</div>
<aside class="col-md-6">
  <section class="user_info">
    <h1><%= @user.name %></h1>

    <p>
      <%= @user.email %>
      <%= @user.delivery_address %> </br>
      <%= @user.cc_number %>

    </p>
  </section>
</aside>
</div>
</div>
